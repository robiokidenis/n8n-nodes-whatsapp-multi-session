{
  "name": "n8n-nodes-whatsapp-multi-session",
  "version": "1.4.1",
  "description": "n8n node for WhatsApp Multi-Session API integration",
  "keywords": [
    "n8n-community-node-package",
    "whatsapp",
    "multi-session",
    "messaging",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/robiokidenis/n8n-nodes-whatsapp-multi-session",
  "author": {
    "name": "robiokidenis",
    "email": "robiokidenis@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robiokidenis/n8n-nodes-whatsapp-multi-session.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=8.1"
  },
  "packageManager": "pnpm@8.1.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts --ext .ts",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WhatsAppMultiSessionApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WhatsAppMultiSession/WhatsAppMultiSession.node.js",
      "dist/nodes/WhatsAppMultiSessionTrigger/WhatsAppMultiSessionTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
